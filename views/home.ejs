    <script>
      function initMap() {
        var lat = 43.653959;
        var lng = -70.263182;
        var center = {lat: lat, lng: lng };
        var map = new google.maps.Map(document.getElementById('homepageMap'), {
            zoom: 12,
            center: center,
            scrollwheel: false
        });
        restaurant.forEach(function(restaurant){
          var restaurantLocation = {lat: restaurant.lat, lng: restaurant.lng};
          var marker = new google.maps.Marker({
            position: restaurantLocation,
            map: map
          });
        });
      }
    </script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCSFnA2kA0xsXzURx0ADLYoV1iJmUA_t0s&callback=initMap"></script>

